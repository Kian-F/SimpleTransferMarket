<%= form_for(@user, :html => {:multipart => true}) do |f| %>
 <%= f.label :name %>
 <%= f.text_field :name, :required => true, :autofocus => true %>
 <%= f.label :email  %>
 <%= f.text_field :email %>
 <%= f.label :phoneNumber  %>
 <%= f.text_field :phoneNumber %>
 <%= f.label :dob, "Date of birth" %>
 <%= f.text_field :dob, :placeholder => 'YYYY-MM-DD' %>
  <%= f.cl_image_upload :image %>
  <%= f.submit %>

<% end %>
